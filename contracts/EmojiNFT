pragma solidity ^0.8.20;
contract EmojiNFT { uint public idx=1; mapping(uint=>string) public emoji; mapping(uint=>address) public owner;
  function mint(string calldata e) external { emoji[idx]=e; owner[idx]=msg.sender; idx++; }
  function read(uint i) external view returns(address,string memory){ return (owner[i],emoji[i]); }
}
