pragma solidity ^0.8.20;
contract MemoToken { mapping(uint=>string) public note; mapping(uint=>address) public owner; uint public n;
  function mint(string calldata t) external { n++; note[n]=t; owner[n]=msg.sender; }
  function get(uint i) external view returns(address,string memory){ return (owner[i],note[i]); }
}
